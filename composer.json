{
	"name": "nette/web-project",
	"description": "Nette: Standard Web Project",
	"keywords": ["nette"],
	"type": "project",
	"license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"require": {
		"php": ">= 8.0",

		"contributte/bootstrap": "^0.6.0",
		"contributte/console": "^0.10.0",
		"contributte/di": "^0.5.6",
		"contributte/http": "^0.4.0",
		"contributte/monolog": "^0.5.0",
		"contributte/neonizer": "^0.5.0",
		"contributte/security": "^0.4.0",
		"contributte/tracy": "^0.6.0",
		"contributte/utils": "^0.6.0",

		"nettrine/orm": "v0.8.4",
		"nettrine/dbal": "v0.8.2",
		"contributte/apitte": "v0.12.1",

		"symfony/serializer": "^6.0.2",
		"symfony/validator": "^7.0.2",
		"symfony/property-info": "^6.0.0",
		"symfony/cache": "^7.0.0",
		"symfony/property-access": "^7.0"
	},
	"require-dev": {
		"nette/tester": "^2.4",
		"symfony/thanks": "^1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
